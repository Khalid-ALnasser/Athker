<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/backgroundneww"

        tools:context=".alhmedlallah">

    <LinearLayout android:layout_width="123dp"
                  android:layout_height="104dp"
                  android:id="@+id/lman"
                  android:orientation="vertical"
                  android:layout_alignParentTop="true"
                  android:layout_marginTop="27dp" android:layout_centerHorizontal="true">

        <TextView android:layout_width="102dp"
                  android:layout_height="100dp"
                  android:id="@+id/TXTCount2"

                  android:background="@drawable/btncercal"
                  android:gravity="center"
                  android:textSize="50dp"
                  android:textStyle="bold"
                  android:fontFamily="@font/digital7"
                  android:ems="10"
                  android:layout_alignParentTop="true"
                  android:layout_alignParentEnd="true"
        />

    </LinearLayout>
    <LinearLayout android:layout_width="123dp"
                  android:layout_height="wrap_content"
                  android:id="@+id/lman51"
                  android:orientation="vertical"
                  android:layout_alignParentEnd="true"
                  android:layout_marginEnd="19dp" android:layout_alignTop="@+id/lbmain99">

        <Button android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/btncs2"
                android:background="@drawable/btn"
                android:textSize="25dp"

                android:text="الحمد لله"/>

    </LinearLayout>
    <LinearLayout android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:id="@+id/lbmain99"
                  android:orientation="vertical"
                  android:layout_alignParentTop="true"
                  android:layout_marginTop="153dp"
                  android:layout_alignParentStart="true"
                  android:layout_marginStart="19dp">
        <Button android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:text="مسح"
                android:textSize="25dp"

                android:background="@drawable/btn"

                android:id="@+id/picrefresh2"

        />

    </LinearLayout>

</RelativeLayout>